        <route type="mix" sink="bus4_usage_voice" sources="mixport_bus4_usage_voice_output,fm_hw,telephony_rx_hw" />
        <route type="mix" sink="telephony_tx" sources="hfp_tx_mix,mixport_bus0_usage_main_output" />
